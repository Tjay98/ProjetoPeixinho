{
    "name": "windwalker/uri",
    "type": "windwalker-package",
    "description": "Windwalker Uri package",
    "keywords": ["windwalker", "framework", "uri"],
    "homepage": "https://github.com/ventoviro/windwalker-uri",
    "license": "LGPL-2.0+",
    "require": {
        "php": ">=5.3.10"
    },
    "require-dev": {
        "windwalker/test": "~3.0",
        "psr/http-message": "1.*"
    },
    "suggest": {
        "psr/http-message": "Install 1.* if you need PsrUri support."
    },
    "minimum-stability": "beta",
    "autoload": {
        "psr-4": {
            "Windwalker\\Uri\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
